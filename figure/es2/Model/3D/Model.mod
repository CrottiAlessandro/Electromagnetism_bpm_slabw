     .Name "hollow part" 
     .Component "component1" 
     .Material "Vacuum" 
     .Xrange "0", "a" 
     .Yrange "0", "b" 
     .Zrange "0", "l" 
     .Create
End With

'@ boolean insert shapes: component1:bulk structure, component1:hollow part

